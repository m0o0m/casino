            }
            else 
            {
                this._frameMode = mgs.aurora.api.enums.SystemButtonTypes.EXPERT;
            }
            this._consoleSaver.save(this._frameMode);
            if (this._frameMode != mgs.aurora.api.enums.SystemButtonTypes.EXPERT) 
            {
                this.showExpertButton();
            }
